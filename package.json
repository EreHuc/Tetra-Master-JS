{
  "name": "tetra-master-js",
  "version": "0.0.1",
  "description": "Recreation of FF9 tetra master game in Javascript",
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel index.html",
    "build": "parcel build index.html",
    "flow": "flow",
    "lint": "eslint src/ --fix",
    "deploy": "rm -rf ./server-deploy/static && parcel build index.html -d server-deploy/static && gcloud app deploy ./server-deploy/app.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EreHuc/Tetra-Master-JS.git"
  },
  "author": "EreHuc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EreHuc/Tetra-Master-JS/issues"
  },
  "homepage": "https://github.com/EreHuc/Tetra-Master-JS#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "flow-bin": "^0.76.0",
    "parcel-bundler": "^1.8.1"
  }
}
